<% if run_step_result.steplevel != nil and run_step_result.steplevel == 'suite'  
 @commentid = nil%>
  <tr expended="" class="tr_result" <% if run_step_result.result.start_with? 'PASS' %>status="pass"<%end%><% if run_step_result.result.start_with? 'FAIL' %>status="fail"<%end%> bgcolor="#AAABA9" id="<%= run_step_result.suite_id %>_<%= run_step_result.suite_sequence %>" name="<%= run_step_result.run_id %>" level='suite'>
      <td>
          <span class="plusmoins" onclick="showhidereportline(this);showbugs();" id="signe<%= run_step_result.suite_id %>_<%= run_step_result.suite_sequence %>"><b>-&nbsp;&nbsp;</b></span>
          <span class="textStyle"><%= t('suite') %> : </span><span  onclick='window.open("../sheets/edit?nw=1&write=1&sheet_id=<%= run_step_result.suite_id %>&load=1&sheettype=test_suite&popup=true", "tests", "directories=0,titlebar=0,toolbar=0,menubar=0,width=1000,height=800");' class="textLink"><%= run_step_result.suite_name %></span>
      </td>
      <td>
          <span class="textStyle"><%= run_step_result.detail %> </span>
      </td>
      <td>
          <span class="textStyle"><%= run_step_result.expected %> </span>
      </td>
      <td <%  if run_step_result.result.start_with? 'PASS' %>bgcolor="#93FA5F"<%end%> 
                <%  if run_step_result.result.start_with? 'FAIL' %>bgcolor="#F96868"<%end%>>
          <span class="textStyle"  ><%= run_step_result.result %> <% if run_step_result.initial_result != nil and run_step_result.result != run_step_result.initial_result %> forced (initial <%= run_step_result.initial_result %>)<% end %> </span>
      </td>
      <td>
          <div class="btnseediagsmall" style="float: left;" title="<%= t('ecr_run.resultat_sur_diag') %>" onclick="show_run_suite_graphe(<%=run_step_result.run_id%>)"></div>
		  <% if @can_do_something == 1 %>
		  <button id='btnlaunch' class='btnlaunch' style='float: right;display:inline-block;' title="<%=  t('relancer') %>" onclick="document.getElementById('contentpopupfiche').innerHTML='<object id=\'objpopup\' width=\'99%\' data=\'../run_configs/testindex?popup=true&type=suite&elem_id=<%= run_step_result.suite_id %>&initrun=<%= run_step_result.run_id %>\'></object>';window.location.href='#popupfiche';document.getElementById('popupfiche').style.display='block';document.getElementById('objpopup').setAttribute('height', 0.89 * document.body.offsetHeight);"></button> 
		  <%end%>
	  </td>
  </tr>
<% end %>