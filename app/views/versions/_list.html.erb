<div id="left">
	<div id="btnresize" class="btnresize" onmousedown="startresizediv(event, this);"></div>
	

	<form method="post">
	<input type="hidden" value="<%= form_authenticity_token() %>" name="authenticity_token"/>
		<table >
		  <tr>
			  <td><input size=20 type="text" spellcheck='false' name="versionsearch" id="versionsearch" value="<%= @versionsearch %>" class="recherche" placeholder="<%= t('input.placeholder.recherche') %>"/></td>
			  <td><input value="" name="search" type="submit" class="btnsearch" vertical-align="middle" onclick="startloader();document.getElementById('versioncre').required=false;"/></td>
		  </tr>
		</table>
		
		<% @versions.each do |version| 
		if version.id != @currentversion %>
		<a class="ligthline" id="version<%=  version.id%>" href="../versions/getone?version_id=<%= version.id %>" onclick='startloader();'><%= version.name %> <img style="float: right;" src="/assets/icones/btnedit1.png" title="<%= t('editer') %>"/></a>
		<% end 
		   end %>
		   
	</form>
    
</div>

<script async type="application/javascript"> 
    <% if @version != nil %>
    surligne_selected("version<%= @version.id %>", "ligthline");
    <% end %>
</script> 